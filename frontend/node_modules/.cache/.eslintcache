Selector } from \"react-redux\";\r\nimport { logout } from \"../actions/userAction\";\r\n\r\nconst Header = () => {\r\n  const userLogin = useSelector((state) => state.userLogin);\r\n  const { userInfo } = userLogin;\r\n  const dispatch = useDispatch();\r\n\r\n  const logoutHandler = () => {\r\n    dispatch(logout());\r\n  };\r\n  return (\r\n    <>\r\n      <nav class=\"navbar navbar-expand-lg navbar-dark bg-primary\">\r\n        <div class=\"container flex\">\r\n          <a class=\"navbar-brand\" href=\"/\">\r\n            Shopping App\r\n          </a>\r\n\r\n          <div class=\"d-flex\">\r\n            <ul class=\"navbar-nav me-auto\">\r\n              <li class=\"nav-item\">\r\n                <a class=\"nav-link\" href=\"/cart\">\r\n                  <i className=\"fas fa-shopping-cart\"></i>\r\n                  &nbsp;CART\r\n                </a>\r\n              </li>\r\n              {userInfo ? (\r\n                <NavDropdown title={userInfo.name} id=\"username\">\r\n                  <Link to=\"/profile\">\r\n                    <NavDropdown.Item>Profile</NavDropdown.Item>\r\n                    <NavDropdown.Item onClick={logoutHandler}>\r\n                      Logout\r\n                    </NavDropdown.Item>\r\n                  </Link>\r\n                </NavDropdown>\r\n              ) : (\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link\" href=\"/login\">\r\n                    <i className=\"fas fa-user\"></i>&nbsp;SIGN IN\r\n                  </a>\r\n                </li>\r\n              )}\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </nav>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Header;\r\n","C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\components\\footer.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\ProductDetails.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\constants\\productConstant.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\ProductScreen.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\components\\Rating.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\components\\shared\\Loader.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\components\\shared\\Message.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\cartScreen.jsx",["111","112","113","114","115","116","117","118","119"],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\reducers\\cartReducers.js",["120","121"],"import { CART_ADD_ITEM, CART_REMOVE_ITEM } from \"../constants/cartConstants\";\r\n\r\nexport const cartReducer = (state = { cartItems: [] }, action) => {\r\n  switch (action.type) {\r\n    case CART_ADD_ITEM:\r\n      const item = action.payload;\r\n      const existItem = state.cartItems.find((x) => x.product == item.product);\r\n      if (existItem) {\r\n        return {\r\n          ...state,\r\n          cartItems: state.cartItems.map((x) =>\r\n            x.product == existItem.product ? item : x\r\n          ),\r\n        };\r\n      } else {\r\n        return { ...state, cartItems: [...state.cartItems, item] };\r\n      }\r\n    case CART_REMOVE_ITEM:\r\n      return {\r\n        ...state,\r\n        cartItems: state.cartItems.filter((x) => x.product !== action.payload),\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n","C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\actions\\cartAction.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\CartScreen.jsx",["122"],"import React, { useEffect } from \"react\";\r\nimport Message from \"../components/shared/Message\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport {\r\n  Row,\r\n  Col,\r\n  Form,\r\n  Button,\r\n  Card,\r\n  Image,\r\n  ListGroup,\r\n  ListGroupItem,\r\n  Container,\r\n} from \"react-bootstrap\";\r\nimport { addToCart, removeFromCart } from \"../actions/cartAction\";\r\n\r\nconst CartScreen = ({ match, location, history }) => {\r\n  const productId = match.params.id;\r\n  const qty = location.search ? Number(location.search.split(\"=\")[1]) : 1;\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(() => {\r\n    if (productId) {\r\n      dispatch(addToCart(productId, qty));\r\n    }\r\n  }, [dispatch, productId, qty]);\r\n\r\n  const cart = useSelector((state) => state.cart);\r\n  const { cartItems } = cart;\r\n  console.log(cartItems);\r\n\r\n  const removeFromCartHandler = (id) => {\r\n    dispatch(removeFromCart(id));\r\n  };\r\n\r\n  const checkout = () => {\r\n    history.push(\"/login?redirect=shipping\");\r\n  };\r\n  return (\r\n    <>\r\n      <Container>\r\n        <Row>\r\n          <h2 className=\"d-flex justify-content-center m-4\">Cart</h2>\r\n\r\n          <Col md={8}>\r\n            {cartItems.length == 0 ? (\r\n              <Message>Your Cart is Empty !</Message>\r\n            ) : (\r\n              <ListGroup variant=\"fluid\">\r\n                {cartItems.map((item) => (\r\n                  <ListGroupItem>\r\n                    <Row className=\"d-flex justify-content-around\">\r\n                      <Col md={2}>\r\n                        <Image src={item.image} alt={item.name} fluid rounded />\r\n                      </Col>\r\n                      <Col md={3}>\r\n                        <Link\r\n                          to={`/product/${item.product}`}\r\n                          style={{ textDecoration: \"none\" }}\r\n                        >\r\n                          {item.name}\r\n                        </Link>\r\n                      </Col>\r\n                      <Col md={2}>Rs. {item.price}</Col>\r\n                      <Col md={2}>\r\n                        <Form.Control\r\n                          as=\"select\"\r\n                          value={item.qty}\r\n                          onChange={(e) =>\r\n                            dispatch(\r\n                              addToCart(item.product, Number(e.target.value))\r\n                            )\r\n                          }\r\n                        >\r\n                          {[...Array(item.countInStock).keys()].map((x) => (\r\n                            <option key={x + 1} value={x + 1}>\r\n                              {x + 1}\r\n                            </option>\r\n                          ))}\r\n                        </Form.Control>\r\n                        <Button\r\n                          type=\"button\"\r\n                          variant=\"light\"\r\n                          onClick={() => removeFromCartHandler(item.product)}\r\n                          className=\"mt-3\"\r\n                        >\r\n                          <i\r\n                            className=\"fa fa-trash text-danger\"\r\n                            aria-hidden=\"true\"\r\n                          ></i>\r\n                        </Button>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroupItem>\r\n                ))}\r\n              </ListGroup>\r\n            )}\r\n          </Col>\r\n          <Col md={4} className=\"\">\r\n            <Card>\r\n              <ListGroup variant=\"flush\">\r\n                <ListGroupItem>\r\n                  <h5>\r\n                    subtotal (\r\n                    {cartItems.reduce((acc, item) => acc + item.qty, 0)}) items\r\n                  </h5>\r\n                  Rs.\r\n                  {cartItems\r\n                    .reduce((acc, item) => acc + item.qty * item.price, 0)\r\n                    .toFixed(2)}\r\n                </ListGroupItem>\r\n                <Button\r\n                  type=\"button\"\r\n                  className=\"btn-block btn-dark btn-md\"\r\n                  disabled={cartItems.length === 0}\r\n                  onClick={checkout}\r\n                >\r\n                  Proceed to checkout\r\n                </Button>\r\n              </ListGroup>\r\n            </Card>\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default CartScreen;\r\n","C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\reducers\\userReducers.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\constants\\userConstants.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\LoginScreen.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\actions\\userAction.js",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\components\\shared\\FormContainer.jsx",[],"C:\\Users\\DELL\\Desktop\\Coding\\E_COMMERCE\\frontend\\src\\screens\\RegisterScreen.jsx",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":2,"column":10,"nodeType":"129","messageId":"130","endLine":2,"endColumn":16},{"ruleId":"127","severity":1,"message":"131","line":2,"column":18,"nodeType":"129","messageId":"130","endLine":2,"endColumn":21},{"ruleId":"127","severity":1,"message":"132","line":2,"column":23,"nodeType":"129","messageId":"130","endLine":2,"endColumn":32},{"ruleId":"127","severity":1,"message":"133","line":2,"column":23,"nodeType":"129","messageId":"130","endLine":2,"endColumn":34},{"ruleId":"127","severity":1,"message":"134","line":4,"column":3,"nodeType":"129","messageId":"130","endLine":4,"endColumn":6},{"ruleId":"127","severity":1,"message":"135","line":5,"column":3,"nodeType":"129","messageId":"130","endLine":5,"endColumn":6},{"ruleId":"127","severity":1,"message":"136","line":6,"column":3,"nodeType":"129","messageId":"130","endLine":6,"endColumn":7},{"ruleId":"127","severity"